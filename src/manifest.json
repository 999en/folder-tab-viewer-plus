{
  "manifest_version": 3,
  "name": "mtab+ New Tab",
  "version": "1.0.0",
  "description": "A minimal new tab page with PDF bookmark support",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage",
    "bookmarks",
    "tabs"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
